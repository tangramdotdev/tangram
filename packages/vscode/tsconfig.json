{
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"exactOptionalPropertyTypes": true,
		"isolatedModules": true,
		"lib": [
			"esnext"
		],
		"module": "commonjs",
		"noEmit": true,
		"noUncheckedIndexedAccess": true,
		"noUncheckedSideEffectImports": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "esnext",
		"types": [
			"node",
			"vscode"
		]
	}
}