{
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"exactOptionalPropertyTypes": true,
		"isolatedModules": true,
		"lib": ["esnext"],
		"module": "esnext",
		"moduleResolution": "bundler",
		"noEmit": true,
		"noUncheckedIndexedAccess": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strict": true,
		"target": "esnext",
		"types": []
	},
	"exclude": ["tangram.d.ts"]
}
